{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "data source=localhost;initial catalog=BuBu;persist security info=True; Integrated Security=SSPI;",
          "tableName": "Log"
        }
      }
    ]
  }
}
